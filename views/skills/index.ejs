<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express Skills</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
    <h1>Skills</h1>
    <ul>
        <% skills.forEach(function(skills) { %>
            <li>
                <form action="/skills/<%= skills.id %>?_method=DELETE"
                    class="delete-form" method="POST">
                    <button type="submit">X</button>
                </form>
                <a href="/skills/<%= skills.frontEnd %>"><%= skills.language %></a>
            </li>
        <% }); %>
    </ul>
  <a href="/skills/new">Add Skill</a>
  <a href="/skills/posts"></a>
</body>
</html>